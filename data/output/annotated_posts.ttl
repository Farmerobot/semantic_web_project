@prefix : <http://example.org/persuasion#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix wd: <http://www.wikidata.org/entity/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

: a owl:Ontology ;
    dc:creator "Semantic Web Project" ;
    dc:description "An ontology for modeling persuasion techniques and misinformation in social media posts" ;
    dc:title "Persuasion-Aware Social Media Ontology" ;
    dcterms:created "2025-01-03"^^xsd:date ;
    owl:versionInfo "2.0" .

:AppealToAuthority a owl:Class ;
    rdfs:label "Appeal to Authority" ;
    rdfs:comment "Citing authority figures to lend credibility without proper evidence" ;
    rdfs:subClassOf :PersuasionTechnique .

:AppealToEmotion a owl:Class ;
    rdfs:label "Appeal to Emotion" ;
    rdfs:comment "Using emotional manipulation instead of logical argument" ;
    rdfs:subClassOf :PersuasionTechnique .

:Bandwagon a owl:Class ;
    rdfs:label "Bandwagon" ;
    rdfs:comment "Appealing to popularity as proof of truth" ;
    rdfs:subClassOf :PersuasionTechnique .

:False a owl:Class ;
    rdfs:label "False" ;
    rdfs:subClassOf :VerificationStatus .

:Location a owl:Class ;
    rdfs:label "Location" ;
    rdfs:comment "A geographical place or region" ;
    rdfs:subClassOf :Entity .

:Misleading a owl:Class ;
    rdfs:label "Misleading" ;
    rdfs:subClassOf :VerificationStatus .

:MostlyFalse a owl:Class ;
    rdfs:label "Mostly False" ;
    rdfs:subClassOf :VerificationStatus .

:MostlyTrue a owl:Class ;
    rdfs:label "Mostly True" ;
    rdfs:subClassOf :VerificationStatus .

:True a owl:Class ;
    rdfs:label "True" ;
    rdfs:subClassOf :VerificationStatus .

:Whataboutism a owl:Class ;
    rdfs:label "Whataboutism" ;
    rdfs:comment "Deflecting criticism by pointing to others' faults" ;
    rdfs:subClassOf :PersuasionTechnique .

:annotatesTechnique a owl:ObjectProperty ;
    rdfs:label "annotates technique" ;
    rdfs:comment "The persuasion technique detected in this annotation" ;
    rdfs:domain :PersuasionAnnotation ;
    rdfs:range :PersuasionTechnique .

:author a owl:DatatypeProperty ;
    rdfs:label "author" ;
    rdfs:domain :Post ;
    rdfs:range xsd:string .

:claimText a owl:DatatypeProperty ;
    rdfs:label "claim text" ;
    rdfs:domain :Claim ;
    rdfs:range xsd:string .

:confidenceScore a owl:DatatypeProperty ;
    rdfs:label "confidence score" ;
    rdfs:comment "LLM confidence in technique detection (0-1)" ;
    rdfs:domain :PersuasionAnnotation ;
    rdfs:range xsd:float .

:containsClaim a owl:ObjectProperty ;
    rdfs:label "contains claim" ;
    rdfs:domain :Post ;
    rdfs:range :Claim .

:contradicts a owl:ObjectProperty,
        owl:SymmetricProperty ;
    rdfs:label "contradicts" ;
    rdfs:comment "Indicates two claims are mutually contradictory" ;
    rdfs:domain :Claim ;
    rdfs:range :Claim .

:correctionText a owl:DatatypeProperty ;
    rdfs:label "correction text" ;
    rdfs:domain :Correction ;
    rdfs:range xsd:string .

:correctsClaim a owl:ObjectProperty ;
    rdfs:label "corrects claim" ;
    rdfs:domain :Correction ;
    rdfs:range :Claim .

:credibilityScore a owl:DatatypeProperty ;
    rdfs:label "credibility score" ;
    rdfs:comment "Assessed credibility of the source (0-1)" ;
    rdfs:domain :Source ;
    rdfs:range xsd:float .

:entityName a owl:DatatypeProperty ;
    rdfs:label "entity name" ;
    rdfs:domain :Entity ;
    rdfs:range xsd:string .

:evidenceSource a owl:DatatypeProperty ;
    rdfs:label "evidence source" ;
    rdfs:domain :Evidence ;
    rdfs:range xsd:anyURI .

:evidenceText a owl:DatatypeProperty ;
    rdfs:label "evidence text" ;
    rdfs:domain :Evidence ;
    rdfs:range xsd:string .

:explanation a owl:DatatypeProperty ;
    rdfs:label "explanation" ;
    rdfs:comment "Explanation for why the technique was detected" ;
    rdfs:domain :PersuasionAnnotation ;
    rdfs:range xsd:string .

:fragmentEnd a owl:DatatypeProperty ;
    rdfs:label "fragment end" ;
    rdfs:comment "End position of claim text fragment" ;
    rdfs:domain :Claim ;
    rdfs:range xsd:integer .

:fragmentStart a owl:DatatypeProperty ;
    rdfs:label "fragment start" ;
    rdfs:comment "Start position of claim text fragment" ;
    rdfs:domain :Claim ;
    rdfs:range xsd:integer .

:hasAnnotation a owl:ObjectProperty ;
    rdfs:label "has annotation" ;
    rdfs:comment "Links a claim to its persuasion annotation" ;
    rdfs:domain :Claim ;
    rdfs:range :PersuasionAnnotation .

:hasCorrection a owl:ObjectProperty ;
    rdfs:label "has correction" ;
    rdfs:domain :Post ;
    rdfs:range :Correction .

:hasImage a owl:DatatypeProperty ;
    rdfs:label "has image" ;
    rdfs:comment "URI reference to an image in the post" ;
    rdfs:domain :Post ;
    rdfs:range xsd:anyURI ;
    rdfs:subPropertyOf :hasContent .

:hasSentiment a owl:ObjectProperty ;
    rdfs:label "has sentiment" ;
    rdfs:comment "The emotional tone of the content" ;
    rdfs:domain :InformationUnit ;
    rdfs:range :Sentiment .

:hasSource a owl:ObjectProperty ;
    rdfs:label "has source" ;
    rdfs:comment "Links content to its information source" ;
    rdfs:domain :InformationUnit ;
    rdfs:range :Source .

:hasText a owl:DatatypeProperty ;
    rdfs:label "has text" ;
    rdfs:comment "The textual content of a post" ;
    rdfs:domain :Post ;
    rdfs:range xsd:string ;
    rdfs:subPropertyOf :hasContent .

:hasVerificationStatus a owl:ObjectProperty ;
    rdfs:label "has verification status" ;
    rdfs:domain :Claim ;
    rdfs:range :VerificationStatus .

:likeCount a owl:DatatypeProperty ;
    rdfs:label "like count" ;
    rdfs:comment "Number of likes/favorites on the post" ;
    rdfs:domain :Post ;
    rdfs:range xsd:integer .

:linkedToWikidata a owl:ObjectProperty ;
    rdfs:label "linked to Wikidata" ;
    rdfs:comment "Links entity to its Wikidata identifier" ;
    rdfs:domain :Entity .

:mentions a owl:ObjectProperty ;
    rdfs:label "mentions" ;
    rdfs:domain :InformationUnit ;
    rdfs:range :Entity .

:modelName a owl:DatatypeProperty ;
    rdfs:label "model name" ;
    rdfs:domain :LLMAgent ;
    rdfs:range xsd:string .

:modelVersion a owl:DatatypeProperty ;
    rdfs:label "model version" ;
    rdfs:domain :LLMAgent ;
    rdfs:range xsd:string .

:platform a owl:DatatypeProperty ;
    rdfs:label "platform" ;
    rdfs:comment "Social media platform (e.g., Twitter, Facebook)" ;
    rdfs:domain :Post ;
    rdfs:range xsd:string .

:postId a owl:DatatypeProperty ;
    rdfs:label "post ID" ;
    rdfs:domain :Post ;
    rdfs:range xsd:string .

:quotes a owl:ObjectProperty ;
    rdfs:label "quotes" ;
    rdfs:comment "Indicates this post quotes another post" ;
    rdfs:domain :Post ;
    rdfs:range :Post .

:refutedBy a owl:ObjectProperty ;
    rdfs:label "refuted by" ;
    rdfs:domain :Claim ;
    rdfs:range :Evidence .

:relatedTo a owl:ObjectProperty,
        owl:SymmetricProperty ;
    rdfs:label "related to" ;
    rdfs:comment "Indicates two claims are topically related" ;
    rdfs:domain :Claim ;
    rdfs:range :Claim .

:replyCount a owl:DatatypeProperty ;
    rdfs:label "reply count" ;
    rdfs:comment "Number of replies to the post" ;
    rdfs:domain :Post ;
    rdfs:range xsd:integer .

:replyTo a owl:ObjectProperty ;
    rdfs:label "reply to" ;
    rdfs:comment "Indicates this post is a reply to another post" ;
    rdfs:domain :Post ;
    rdfs:range :Post .

:sentimentScore a owl:DatatypeProperty ;
    rdfs:label "sentiment score" ;
    rdfs:comment "Numerical sentiment polarity (-1 to 1)" ;
    rdfs:domain :InformationUnit ;
    rdfs:range xsd:float .

:shareCount a owl:DatatypeProperty ;
    rdfs:label "share count" ;
    rdfs:comment "Number of times the post was shared/retweeted" ;
    rdfs:domain :Post ;
    rdfs:range xsd:integer .

:supportedBy a owl:ObjectProperty ;
    rdfs:label "supported by" ;
    rdfs:comment "Links a claim to supporting or refuting evidence" ;
    rdfs:domain :Claim ;
    rdfs:range :Evidence .

:targetsEntity a owl:ObjectProperty ;
    rdfs:label "targets entity" ;
    rdfs:comment "Identifies the entity that is the subject of manipulation" ;
    rdfs:domain :Claim ;
    rdfs:range :Entity .

:timestamp a owl:DatatypeProperty ;
    rdfs:label "timestamp" ;
    rdfs:range xsd:dateTime .

:usesTechnique a owl:ObjectProperty ;
    rdfs:label "uses technique" ;
    rdfs:comment "Links a claim to the persuasion technique it employs" ;
    rdfs:domain :Claim ;
    rdfs:range :PersuasionTechnique .

:wasGeneratedBy a owl:ObjectProperty ;
    rdfs:label "was generated by" ;
    rdfs:comment "Links data to the process/agent that generated it" ;
    owl:equivalentProperty prov:wasGeneratedBy .

:wikidataId a owl:DatatypeProperty ;
    rdfs:label "Wikidata ID" ;
    rdfs:domain :Entity ;
    rdfs:range xsd:string .

